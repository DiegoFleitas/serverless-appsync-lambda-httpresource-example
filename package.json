  "scripts": {
    "dynamodb-install": "yarn serverless dynamodb install --localPath ./bin",
    "dev": "SLS_DEBUG=* NODE_DEBUG=appsync-* yarn sls offline start",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "pretest": "node dynamodb-local.js",
    "test": "jest --detectOpenHandles",
    "test:dev": "yarn test --watch",
    "sls:deploy": "sls deploy",
    "sls:remove": "sls remove",
    "docker:build": "docker build -t serverless-appsync-lambda-httpresource-example .",
    "docker:test": "docker run -v $PWD/:/root/app/ -v node_modules:/root/app/node_modules -ti --rm serverless-appsync-lambda-httpresource-example:latest yarn test",
    "docker:test:dev": "docker run -v $PWD/:/root/app/ -v node_modules:/root/app/node_modules -ti --rm serverless-appsync-lambda-httpresource-example:latest yarn test:dev",
    "docker:dev": "docker run -p 3001:3001 -p 62222:62222 -v $PWD/:/root/app/ -v node_modules:/root/app/node_modules -ti --rm serverless-appsync-lambda-httpresource-example:latest yarn dev"
  },
  "dependencies": {
    "graphql-tag": "^2.10.1",
    "node-fetch": "^2.6.0",
    "serverless-appsync-plugin": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@conduitvc/appsync-emulator-serverless": "^0.14.5",
    "aws-appsync": "^2.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^4.1.2",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "serverless": "^1.53.0",
    "serverless-appsync-offline": "^1.3.3",
    "serverless-offline": "^5.12.0"
  },
  "resolutions": {
    "@hapi/subtext": "^6.1.2",
    "bl": "^1.0.1",
    "handlebars": "^4.3.0",
    "lodash": "^4.17.12"
  }
